<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%- include('layout/head') %>
  <title>Homepage</title>
</head>
<body>

  <%- include('layout/header') %>


  <h1>Home Page</h1>
  <% if (users) { %>
    <h1>All Users</h1>

    <% users.forEach(user => { %>
      <h3>Username: <%= user.username %></h3>
      <h3>Email: <%= user.email %></h3>
   <% }) %>
  <%}%>

  <%- include('layout/footer') %>

  
</body>
</html>